	       $('#checksin').click(function(){
		      i++;
		         $('#dynamic_field_checksin').append('<tr id="row'+i+'"><td><input type="text" name="checksin1[]" placeholder="Bank Details" class="form-control" /><td><input type="text" name="checksin2[]" placeholder="Check Number" class="form-control" /></td><td><input type="date" name="checksin3[]" placeholder="Date" class="form-control" /><td><input type="text" name="checksin4[]" placeholder="Name" class="form-control" /></td><td><input type="text" name="checksin5[]" placeholder="Amount" class="form-control inst_amount_checksin" /></td><td><button type="button" name="remove" id="'+i+'" class="btn btn-danger btn_remove">X</button></td></tr>');
	       });
    
           
         //CHECKS OUT DYNAMIC ADD    
         $('#checksout').click(function(){
             i++;
                $('#dynamic_field_checksout').append('<tr id="row'+i+'"><td><input type="text" name="checksout1[]" placeholder="Bank Details" class="form-control" /></td><td><input type="text" name="checksout2[]" placeholder="Check Number" class="form-control" /></td><td><input type="date" name="checksout3[]" placeholder="date" class="form-control" /></td></td><td><input type="text" name="checksout4[]" placeholder="Name" class="form-control" /></td><td><input type="text" name="checksout5[]" placeholder="Amount" class="form-control inst_amount_checksout" /></td><td><button type="button" name="remove" id="'+i+'" class="btn btn-danger btn_remove">X</button></td></tr>');
	     });    
            
            
            
            
            
        
    
            
	   $(document).on('click', '.btn_remove', function(){
		var button_id = $(this).attr("id"); 
		$('#row'+button_id+'').remove();
	   });
	
        });
       </script>
       <script>
        $(function () {
             $("#totalMorning, #oneKMorning, #fiveHMorning, #oneHMorning, #fiftyMorning, #twentyMorning, #tenMorning, #fiveMorning, #oneMorning, #tfiveMorning, #totalEvening, #oneKEvening, #fiveHEvening, #oneHEvening, #fiftyEvening, #twentyEvening, #tenEvening, #fiveEvening, #oneEvening, #tfiveEvening, #totalsecMorning, #oneKsecMorning, #fiveHsecMorning, #oneHsecMorning, #fiftysecMorning, #twentysecMorning, #tensecMorning, #fivesecMorning, #onesecMorning, #tfivesecMorning, #totalsecEvening, #oneKsecEvening, #fiveHsecEvening, #oneHsecEvening, #fiftysecEvening, #twentysecEvening, #tensecEvening, #fivesecEvening, #onesecEvening, #tfivesecEvening, #totalIn, #totalOut, #totalAllMorning, #totalAllEvening, #wsTotal, #scale, #sobra, #totalSale, #difference, #totalsales1, #totalsales2, #totalsales").keyup(function () {
                  $("#totalMorning").val(+$("#oneKMorning").val() + +$("#fiveHMorning").val() + +$("#oneHMorning").val() + +$("#fiftyMorning").val() + +$("#twentyMorning").val() + +$("#tenMorning").val() + +$("#fiveMorning").val() + +$("#oneMorning").val() + +$("#tfiveMorning").val());
                 
                  $("#totalEvening").val(+$("#oneKEvening").val() + +$("#fiveHEvening").val() + +$("#oneHEvening").val() + +$("#fiftyEvening").val() + +$("#twentyEvening").val() + +$("#tenEvening").val() + +$("#fiveEvening").val() + +$("#oneEvening").val() + +$("#tfiveEvening").val());   
                 
                  $("#totalsecMorning").val(+$("#oneKsecMorning").val() + +$("#fiveHsecMorning").val() + +$("#oneHsecMorning").val() + +$("#fiftysecMorning").val() + +$("#twentysecMorning").val() + +$("#tensecMorning").val() + +$("#fivesecMorning").val() + +$("#onesecMorning").val() + +$("#tfivesecMorning").val());
                    
                 $("#totalsecEvening").val(+$("#oneKsecEvening").val() + +$("#fiveHsecEvening").val() + +$("#oneHsecEvening").val() + +$("#fiftysecEvening").val() + +$("#twentysecEvening").val() + +$("#tensecEvening").val() + +$("#fivesecEvening").val() + +$("#onesecEvening").val() + +$("#tfivesecEvening").val());
                 
                 $("#totalAllMorning").val(+$("#totalMorning").val() + +$("#totalsecMorning").val());
                 $("#totalAllEvening").val(+$("#totalEvening").val() + +$("#totalsecEvening").val());
                 
                
                 
                 $("#wsTotal").val(+$("#scale").val() - +$("#sobra").val());
                 $("#difference").val(+$("#scale").val() - +$("#totalSale").val());
                 $("#totalSale").val(+$("#totalIn").val() + +$("#totalOut").val());
                 
                 
                 
                 
             });
         });            
       </script>
     


    </body>
</html>
